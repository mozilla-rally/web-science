{
  "description": "A test extension for the WebScience library.",
  "author": "Mozilla",
  "manifest_version": 2,
  "name": "WebScience Test",
  "version": "1.0.0",
  "homepage_url": "https://github.com/mozilla-rally/web-science",
  "browser_specific_settings": {
    "gecko": {
      "id": "webScience-test@mozilla.org",
      "strict_min_version": "87.0"
    }
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "permissions": [
    "idle",
    "webNavigation",
    "webRequest",
    "unlimitedStorage",
    "scripting",
    "<all_urls>"
  ],
  "incognito": "not_allowed"
}