{
  "description": "A test extension for the WebScience library.",
  "author": "Mozilla",
  "manifest_version": 3,
  "name": "WebScience Test",
  "version": "1.0.0",
  "homepage_url": "https://github.com/mozilla-rally/web-science",
  "background": {
    "service_worker": "dist/background-loader.js"
  },
  "permissions": [
    "idle",
    "webNavigation",
    "webRequest",
    "unlimitedStorage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "not_allowed"
}